{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parser": "@typescript-eslint/parser",
	"plugins": ["prettier", "@typescript-eslint"],
	"parserOptions": { "ecmaVersion": 2020 },
	"rules": {
		"prettier/prettier": "error",
		"camelcase": ["error"],
		"eqeqeq": ["error", "always"],
		"no-unused-vars": "off"
	},
	"root": true
}
